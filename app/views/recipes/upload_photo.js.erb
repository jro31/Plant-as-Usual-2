$('#spinner').addClass('d-none');
<% if @result == 'success' %>
  $('#photo-container').children(":first").replaceWith(`<%= image_tag @recipe.photo.url, class: "recipe-photo #{@user_can_edit ? 'cursor-pointer' : ''}" %>`)
  displayHiddenFlash('Photo uploaded', 'success')
  recipeHasBeenEdited = true
  displayMarkAsCompleteButton()
  setPhotoUploader()
<% end %>
<% if @result == 'fail' %>
  displayHiddenFlash('Unable to upload photo', 'fail')
<% end %>
